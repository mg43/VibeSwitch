{
  "expo": {
    "name": "Vibe Shift",
    "description": "An app designed to shift the tone of your text by age.",
    "owner": "mg43",
    "slug": "vibe-shift",
    "version": "1.0.0",
    "sdkVersion": "54.0.21",
    "orientation": "portrait",
    "scheme": "vibe-shift",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["android"],
    "backgroundColor": "#afeeee",
    "primaryColor": "#afeeee",
    "android": {
      "package": "com.anonymous.VibeSwitch",
      "icon": "/home/mitch/code/personal/VibeShift/assets/images/vibeswitch-icon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "extra": {
      "eas": {
        "projectId": "ca4c4fab-4cf1-4fdf-bbd0-3f0342385bc1"
      }
    },
    "plugins": [
      "expo-font",
      [
        "llama.rn",
        {
          "enableEntitlements": true,
          "entitlementsProfile": "production",
          "forceCxx20": true,
          "enableOpenCL": true
        }
      ]
    ]
  }
}
